{
    "name": "AutoAccept",
    "version": "1.0",
    "description": "Auto accept dialog periodically",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/reload_ok16.png",
          "32": "images/reload_ok32.png",
          "48": "images/reload_ok48.png",
          "128": "images/reload_ok128.png"
        }
    },
    "options_page": "options.html",
    "icons": {
      "16": "images/reload_ok16.png",
      "32": "images/reload_ok32.png",
      "48": "images/reload_ok48.png",
      "128": "images/reload_ok128.png"
    },
    "manifest_version": 2
}
